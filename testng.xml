<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="LogIn_LogOut">
	<listeners>
		<listener class-name="listeners.Listeners"></listener>
	</listeners>
  <test thread-count="5" name="Login_Test">
    <classes>
      <class name="tests.LoginTest">
		  <methods>
			  <include name="login"/>
			  <include name="logout"/>
		  </methods>
      </class>
    </classes>
  </test> <!-- Login_Test -->
</suite> <!-- LogIn_LogOut -->
